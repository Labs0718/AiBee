{
  "family": "suna-frontend-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::265766434511:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "suna-frontend",
      "image": "265766434511.dkr.ecr.ap-northeast-2.amazonaws.com/suna-frontend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NEXT_PUBLIC_ENV_MODE",
          "value": "production"
        },
        {
          "name": "NEXT_PUBLIC_SUPABASE_URL",
          "value": "https://oupvawyejyteixggrfat.supabase.co"
        },
        {
          "name": "NEXT_PUBLIC_SUPABASE_ANON_KEY",
          "value": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6Im91cHZhd3llanl0ZWl4Z2dyZmF0Iiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTUwNjM5NTUsImV4cCI6MjA3MDYzOTk1NX0.0B1PlWkBFW-QFVk2yVtMVyyWbXYWUeO8a48FDZvcFdA"
        },
        {
          "name": "NEXT_PUBLIC_BACKEND_URL",
          "value": "http://suna-backend-service.aibee-cluster:8000/api"
        },
        {
          "name": "NEXT_PUBLIC_URL",
          "value": "http://localhost:3000"
        }
      ],
      "secrets": [
        {
          "name": "SUPABASE_SERVICE_ROLE_KEY",
          "valueFrom": "arn:aws:ssm:ap-northeast-2:265766434511:parameter/suna/supabase/service-role-key"
        },
        {
          "name": "OPENAI_API_KEY",
          "valueFrom": "arn:aws:ssm:ap-northeast-2:265766434511:parameter/suna/openai/api-key"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/suna-frontend",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost:3000 || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}